{
  "name": "starter_vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,vue --ignore-path .gitignore .",
    "test": "vitest --environment jsdom --root src/",
    "test:coverage": "vitest --environment jsdom --root src/ --coverage",
    "test:coverage++": "vitest --environment jsdom --root src/ --coverage --coverage.lines=80",
    "test:ui": "vitest --environment jsdom --root src/ --coverage.enabled=true --ui"
  },
  "dependencies": {
    "axios": "^1.7.4",
    "pinia": "^2.2.1",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@types/node": "^22.3.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "happy-dom": "^14.12.3",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^2.0.5",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.29"
  }
}
