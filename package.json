{
  "name": "starter_vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,vue --ignore-path .gitignore .",
    "test": "vitest --environment jsdom --root src/",
    "test:coverage": "vitest --environment jsdom --root src/ --coverage",
    "test:coverage++": "vitest --environment jsdom --root src/ --coverage --coverage.lines=80",
    "test:ui": "vitest --environment jsdom --root src/ --coverage.enabled=true --ui"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "pinia": "^2.1.7",
    "vue": "^3.4.23",
    "vue-router": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^14.7.1",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sass": "^1.75.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^5.2.9",
    "vitest": "^1.5.0",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^2.0.13"
  }
}
